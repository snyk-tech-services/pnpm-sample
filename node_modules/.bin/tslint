#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/antoine/Documents/SnykTSDev/Playgrounds/pnpm/node_modules/.pnpm/registry.npmjs.org/tslint/5.1.0/node_modules/tslint/bin/tslint/node_modules:/home/antoine/Documents/SnykTSDev/Playgrounds/pnpm/node_modules/.pnpm/registry.npmjs.org/tslint/5.1.0/node_modules/tslint/bin/node_modules:/home/antoine/Documents/SnykTSDev/Playgrounds/pnpm/node_modules/.pnpm/registry.npmjs.org/tslint/5.1.0/node_modules/tslint/node_modules:/home/antoine/Documents/SnykTSDev/Playgrounds/pnpm/node_modules/.pnpm/registry.npmjs.org/tslint/5.1.0/node_modules:/home/antoine/Documents/SnykTSDev/Playgrounds/pnpm/node_modules/.pnpm/registry.npmjs.org/tslint/node_modules:/home/antoine/Documents/SnykTSDev/Playgrounds/pnpm/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/antoine/Documents/SnykTSDev/Playgrounds/pnpm/node_modules/.pnpm/node_modules:/home/antoine/Documents/SnykTSDev/Playgrounds/pnpm/node_modules:/home/antoine/Documents/SnykTSDev/Playgrounds/node_modules:/home/antoine/Documents/SnykTSDev/node_modules:/home/antoine/Documents/node_modules:/home/antoine/node_modules:/home/node_modules:/node_modules:/home/antoine/Documents/SnykTSDev/Playgrounds/pnpm/node_modules/tslint/bin/tslint/node_modules:/home/antoine/Documents/SnykTSDev/Playgrounds/pnpm/node_modules/tslint/bin/node_modules:/home/antoine/Documents/SnykTSDev/Playgrounds/pnpm/node_modules/tslint/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tslint/bin/tslint" "$@"
else 
  exec node  "$basedir/../tslint/bin/tslint" "$@"
fi
